  <description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Break down ROE into profit margin, asset turnover, and equity multiplier.</description>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#RetailChainExpansion">
  <rdf:type rdf:resource="#CaseStudy"/>
