<owl:NamedIndividual rdf:about="#LiquidityRatios">
  <rdf:type rdf:resource="#Concept"/>
  <relatedTo rdf:resource="#ProfitabilityRatios"/>
  <hasPractice rdf:resource="#LiquidityExercise2"/>
  <hasPractice rdf:resource="#LiquidityExercise1"/>
  <hasCaseStudy rdf:resource="#RetailChainExpansion"/>
  <hasQuiz rdf:resource="#LiquidityQ1"/>
  <hasTheory rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Liquidity ratios measure a company's ability to cover short-term obligations.</hasTheory>
  <hasExample rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Current Ratio = Current Assets / Current Liabilities</hasExample>
  <hasLevel rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Beginner</hasLevel>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#ProfitabilityRatios">
  <rdf:type rdf:resource="#Concept"/>
  <relatedTo rdf:resource="#LiquidityRatios"/>
  <relatedTo rdf:resource="#DuPontAnalysis"/>
  <hasPractice rdf:resource="#ProfitabilityExercise1"/>
  <hasCaseStudy rdf:resource="#ManufacturingTurnaround"/>
  <hasQuiz rdf:resource="#ProfitabilityQ1"/>
  <hasTheory rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Profitability ratios measure the company's ability to generate profit relative to revenue, assets, or equity.</hasTheory>
  <hasExample rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Net Profit Margin = Net Income / Revenue</hasExample>
  <hasLevel rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Intermediate</hasLevel>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#DuPontAnalysis">
  <rdf:type rdf:resource="#Concept"/>
  <relatedTo rdf:resource="#ProfitabilityRatios"/>
  <hasPractice rdf:resource="#DuPontExercise1"/>
  <hasCaseStudy rdf:resource="#ManufacturingTurnaround"/>
  <hasQuiz rdf:resource="#DuPontQ1"/>
  <hasTheory rdf:datatype="http://www.w3.org/2001/XMLSchema#string">DuPont analysis decomposes ROE into multiple components for deeper insight.</hasTheory>
  <hasExample rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ROE = Profit Margin * Asset Turnover * Equity Multiplier</hasExample>
  <hasLevel rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Advanced</hasLevel>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#LiquidityExercise1">
  <rdf:type rdf:resource="#PracticeExercise"/>
  <description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Compute the current ratio for a sample company.</description>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#LiquidityExercise2">
  <rdf:type rdf:resource="#PracticeExercise"/>
  <description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Compute the quick ratio for a sample company.</description>
