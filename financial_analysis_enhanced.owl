<owl:NamedIndividual rdf:about="#ProfitabilityExercise1">
  <rdf:type rdf:resource="#PracticeExercise"/>
  <description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Calculate net profit margin using given financial statements.</description>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#DuPontExercise1">
