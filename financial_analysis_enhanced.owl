  <description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Assess the financial viability of expansion plans using liquidity ratios.</description>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#ManufacturingTurnaround">
  <rdf:type rdf:resource="#CaseStudy"/>
  <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Manufacturing Company Turnaround</title>
  <description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Evaluate restructuring success through profitability and DuPont analysis.</description>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#LiquidityQ1">
  <rdf:type rdf:resource="#QuizQuestion"/>
  <questionText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">What is the formula for current ratio?</questionText>
  <options rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Current Assets / Current Liabilities | Net Income / Total Assets | Cash / Debt</options>
  <correctAnswer rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Current Assets / Current Liabilities</correctAnswer>
  <difficulty rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Beginner</difficulty>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#ProfitabilityQ1">
  <rdf:type rdf:resource="#QuizQuestion"/>
  <questionText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Net Profit Margin is calculated as?</questionText>
  <options rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Net Income / Revenue | Revenue / Total Assets | Operating Income / Equity</options>
  <correctAnswer rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Net Income / Revenue</correctAnswer>
  <difficulty rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Intermediate</difficulty>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#DuPontQ1">
  <rdf:type rdf:resource="#QuizQuestion"/>
  <questionText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ROE in DuPont analysis is decomposed into which components?</questionText>
  <options rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Profit Margin * Asset Turnover * Equity Multiplier | Revenue / Assets | Net Income / Debt</options>
  <correctAnswer rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Profit Margin * Asset Turnover * Equity Multiplier</correctAnswer>
  <difficulty rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Advanced</difficulty>
</owl:NamedIndividual>


